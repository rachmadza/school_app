<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>Data Subject</title>
  </head>
  <body>
    <h1>Subjects</h1>
    <ul>
      <%data.forEach(function(subject) {%>
      <li>
        Subject : <%=subject.subject_name %>
        <br>
        Teacher : 
        <br>
          <% subject.dataValues.Teacher.forEach((teacher,index) => {%>

              <%=index+1%>. <%=teacher.getFullName() %> <br>

            <% }) %>
        <br>
        <a href="subjects/edit/<%=subject.id%>">Edit</a>
        <a href="subjects/delete/<%=subject.id%>" onclick="return confirm('Anda yakin ingin menghapus data ini?')">Delete</a>
      </li>
      <br>
      <%})%>
      <br>
      <a href="subjects/add">Add Subject</a>
    </ul>
  </body>
</html>